                <div class="bubble-Out-text">OUTERWEAR</div>
            </div>
        </div>
            <div class="mrow-img-container">
                <% for (let j=0; j < product.length; j++) { if (j % 4===0) { <!-- Start a new row every 4 items -->
                    %>
                    <div class="row">
                        <% } %>
                            <div class="col-4 m-3">
                                <div class="card" style="width: 18rem; position: relative; overflow: hidden;">
                                    <div class="img-hover-container">
                                        <img class="Frag" src="<%= product[j].src %>" alt="Card image cap">
                                        <a href="/add-to-cart/<%= product[j]._id %>"
                                            class="btn btn-primary hover-cart-btn">Add To Cart</a>
                                    </div>
                                    <div class="card-body">
                                        <h5 class="card-title">
                                            <%= product[j].title %>
                                        </h5>
                                        <p class="card-description">
                                            <%= product[j].description %>
                                        </p>
                                        <p class="card-text">PKR: <%= product[j].price %>
                                        </p>
                                    </div>
                                </div>
                            </div>

                            <% if ((j + 1) % 4===0 || j===product.length - 1) { <!-- Close the row -->
                                %>
                    </div>
                    <% } } %>
            </div>
        </div>
    </div>
    <div id="bottom-frag-container">
        <div class="bottom-top-text">Buy Perfumes for Men: Fragrances for Men in Pakistan Online</div>
        <p class="bottom-para1">Step up your fragrance game with our elite collection of perfumes for men. Designed
